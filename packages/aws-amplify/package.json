{
  "name": "aws-amplify",
  "version": "2.0.0-preview.0",
  "description": "AWS Amplify is a JavaScript library for Frontend and mobile developers building cloud-enabled applications.",
  "main": "./index.js",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "react-native": {
    "./index": "./esm/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "test": "tslint 'src/**/*.ts' && jest --coverage --maxWorkers 2",
    "build-with-test": "npm run clean && npm test && tsc && webpack -p",
    "build:cjs": "node ./build es5 && webpack && webpack --config ./webpack.config.dev.js",
    "build:esm": "node ./build es6",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "clean": "rimraf esm cjs dist",
    "format": "tsfmt --useTsfmt tsfmt.json -r src/**/*.ts",
    "lint": "tslint 'src/**/*.ts'",
    "generate-docs-local": "typedoc --out docs src",
    "generate-docs-root": "typedoc --out ../../docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aws/aws-amplify.git"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aws/aws-amplify/issues"
  },
  "homepage": "https://github.com/aws/aws-amplify#readme",
  "devDependencies": {
  },
  "dependencies": {
    "@aws-amplify/analytics": "2.0.x",
    "@aws-amplify/api": "2.0.x",
    "@aws-amplify/auth": "2.0.x",
    "@aws-amplify/cache": "2.0.x",
    "@aws-amplify/core": "2.0.x",
    "@aws-amplify/interactions": "2.0.x",
    "@aws-amplify/pubsub": "2.0.x",
    "@aws-amplify/storage": "2.0.x",
    "@aws-amplify/ui": "2.0.x",
    "@aws-amplify/xr": "1.0.x"
  }
}
